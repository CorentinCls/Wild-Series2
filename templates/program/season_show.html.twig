{% extends "base.html.twig" %}

{% block body %}

<h1>Episodes :</h1>

<ul>
    {% for episode in season.episodes %}
        <li><a href="{{ path('program_episode_show', { program: program.id, season : season.id, episode: episode.id }) }}"> {{episode.title}} </a></li>
    {% endfor %}
</ul>


{% endblock %}