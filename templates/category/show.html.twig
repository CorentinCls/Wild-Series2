{% extends 'base.html.twig' %}

{% block title %}Hello CategoryController!{% endblock %}

{% block body %}
    <ul>
        {% for program in programs %}
            <li>
                <a href="{{ path('program_show', { id: program.id }) }}">{{program.title}}</a>
            </li>
        {% endfor %}
    </ul>
    
{% endblock %}